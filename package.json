{
  "name": "vfg-field-matrix",
  "version": "0.0.3",
  "private": false,
  "main": "dist/field-matrix.umd.js",
  "module": "dist/field-matrix.esm.js",
  "unpkg": "dist/field-matrix.min.js",
  "browser": {
    "./sfc": "src/field-matrix.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config config/rollup.config.js --format umd --file dist/field-matrix.umd.js",
    "build:es": "rollup --config config/rollup.config.js --format es --file dist/field-matrix.esm.js",
    "build:unpkg": "rollup --config config/rollup.config.js --format iife --file dist/field-matrix.min.js",
    "lint": "vue-cli-service lint",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "serve:storybook": "vue-cli-service serve:storybook -p 6006 -c config/storybook",
    "test:unit": "vue-cli-service test:unit",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/github": "^5.2.5",
    "@semantic-release/npm": "^5.1.1",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "bootstrap": "^4.1.3",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.9.0",
    "rollup": "^0.67.4",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.0.1",
    "semantic-release": "^15.12.4",
    "vue": "^2.5.17",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-form-generator": "^2.3.2",
    "vue-template-compiler": "^2.5.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shwld/vfg-field-matrix.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
